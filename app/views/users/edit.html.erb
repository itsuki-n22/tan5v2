<% @title = "アカウント情報" %>

<div id="edit-user-form">
  <h4 id="edit-user-form-title"><%= @title %></h4>
  <%= form_with model: @user, url: :user, local: true do |f| %>
    <%= render "form", f: f %>
  <% end %>
  <div><%= link_to "ログアウト", :session, method: :delete, class: "logout-btn btn btn-outline-info" %></div>
</div>
